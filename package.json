{
  "name": "banned-historical-archives.github.io",
  "version": "0.1.0",
  "scripts": {
    "dev:nextjs-helper": "ts-node --project ./tsconfig.build.json ./backend/nextjs-helper &",
    "build-catelog": "ts-node --project ./tsconfig.build.json ./backend/build-catelog.ts",
    "dev": "COMMIT_HASH=$(git rev-parse HEAD) next dev",
    "init-config": "ts-node --project ./tsconfig.build.json ./backend/init-sub-repository.ts config",
    "init-parsed": "ts-node --project ./tsconfig.build.json ./backend/init-sub-repository.ts parsed",
    "init-raw": "ts-node --project ./tsconfig.build.json ./backend/init-sub-repository.ts raw",
    "init-ocr_cache": "ts-node --project ./tsconfig.build.json ./backend/init-sub-repository ocr_cache",
    "init-ocr_patch": "ts-node --project ./tsconfig.build.json ./backend/init-sub-repository ocr_patch",
    "init-es": "ts-node --project ./tsconfig.build.json ./backend/init-es.ts",
    "hack-mui": "sed -i'' -e 's/return React/return LicenseStatus.Valid;return React/g' node_modules/@mui/x-license-pro/useLicenseVerifier/useLicenseVerifier.js",
    "build": "npm run hack-mui && next build",
    "build-txt": "ts-node --project ./tsconfig.build.json ./backend/build-txt.ts",
    "cp-pdfjs": "cp -r ./node_modules/pdfjs-dist ./public/",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier -w ."
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.18.9",
    "@elastic/elasticsearch": "^8.5.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@mui/system": "^5.11.0",
    "@mui/x-data-grid": "^5.17.0",
    "@mui/x-data-grid-pro": "^5.17.0",
    "@types/connect-timeout": "^0.0.36",
    "@types/diff": "^5.0.2",
    "@types/express": "^4.17.13",
    "axios": "^0.27.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "dbffile-fix-decoding": "^1.8.1",
    "diff-match-patch": "^1.0.5",
    "dommatrix": "^1.0.3",
    "dotenv": "^16.0.3",
    "epub": "^1.2.1",
    "express": "^4.18.1",
    "fast-xml-parser": "^4.3.2",
    "file-type": "^17.0.0",
    "file-type-cjs": "^1.0.7",
    "fs-extra": "^10.1.0",
    "iconv-lite": "^0.6.3",
    "image-size": "^1.0.2",
    "jsdom": "^20.0.0",
    "json5": "^2.2.2",
    "memo_file": "^1.1.0",
    "mysql2": "^2.3.3",
    "next": "^14.0.4",
    "next-sitemap": "^3.1.13",
    "node-fetch": "^2.6.7",
    "pdf.js": "^0.1.0",
    "pdfjs-dist": "4.2",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "react-pdf": "^7.7.3",
    "remark-gfm": "^3.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.7",
    "uuid": "^9.0.0",
    "vitest": "^0.19.1",
    "web-streams-polyfill": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/plugin-syntax-export-default-from": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cors": "^2.8.12",
    "@types/diff-match-patch": "^1.0.32",
    "@types/fs-extra": "^9.0.13",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-pdf": "^5.7.2",
    "@types/uuid": "^8.3.4",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "prettier": "2.8.3",
    "typescript": "^4.9.5"
  }
}
