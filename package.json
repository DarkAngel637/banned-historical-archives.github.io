{
  "name": "banned-history",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:build-static": "ts-node --project ./tsconfig.node.json ./backend/build-static.ts",
    "init-db": "ts-node --project ./tsconfig.node.json ./backend/init-db.ts",
    "dev:prepare": "cp -r ./node_modules/pdfjs-dist /public/pdfjs-dist",
    "dev": "npm run dev:prepare && vite",
    "build-static": "PRODUCTION_MODE=true ts-node --project ./tsconfig.node.json ./backend/build-static.ts",
    "test": "vitest",
    "build": "vite build && cp -r ./node_modules/pdfjs-dist ./dist/pdfjs-dist && npm run build-static && cp -r ./public/books ./dist/books",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.7",
    "@mui/x-data-grid": "^5.13.0",
    "@mui/x-date-pickers": "^5.0.0-beta.0",
    "@types/diff": "^5.0.2",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "diff": "^5.1.0",
    "dommatrix": "^1.0.3",
    "express": "^4.18.1",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.7",
    "pdf.js": "^0.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-pdf": "^5.7.2",
    "react-router-dom": "^6.3.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.7",
    "web-streams-polyfill": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.29",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-pdf": "^5.7.2",
    "@vitejs/plugin-react": "^1.3.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.9",
    "vitest": "^0.18.0"
  }
}
